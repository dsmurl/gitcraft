{
  "name": "gitcraft",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@9.14.2",
  "scripts": {
    "dev": "pnpm run stack:dev",
    "stack:dev": "nx run devstack:dev",
    "build": "nx run-many -t build --parallel",
    "build:web": "nx run web:build",
    "build:api": "nx run api:build",
    "lint": "nx run web:lint",
    "preview": "nx run web:preview",
    "web:dev": "nx run web:dev",
    "web:build": "nx run web:build",
    "web:lint": "nx run web:lint",
    "web:preview": "nx run web:preview",
    "api:dev": "nx run api:dev",
    "api:build": "nx run api:build",
    "api:start": "nx run api:start",
    "drizzle:dev": "nx run drizzle:dev",
    "drizzle:build": "nx run drizzle:build",
    "db:init": "nx run drizzle:db:init",
    "db:generate": "nx run drizzle:db:generate",
    "db:migrate": "nx run drizzle:db:migrate",
    "db:studio": "nx run drizzle:db:studio",
    "prettier": "prettier --write .",
    "nx:reset": "nx reset",
    "clean:dist": "rm -rf apps/*/dist libs/*/dist apps/*/.vite .nx node_modules/.cache",
    "clean:modules": "rm -rf  apps/*/node_modules libs/*/node_modules node_modules",
    "clean": "pnpm run nx:reset && pnpm run clean:dist",
    "clean:all": "pnpm run clean && pnpm run clean:modules && pnpm store prune"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.47.0",
    "@tailwindcss/vite": "^4.1.13",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "nx": "^20.1.0",
    "prettier": "^3.6.2",
    "vite": "^7.1.2"
  }
}
